<?xml version="1.0" encoding="UTF-8"?>

<tdengine>
    <!--    http or sdk    -->
    <type>http</type>
    <!--    use hostname when using sdk   -->
    <ip>127.0.0.1</ip>
    <!--    http 6041  sdk 6030   -->
    <port>6041</port>
    <!--    default root   -->
    <username>root</username>
    <!--    default taosdata   -->
    <password>taosdata</password>
    <!--    database name default hivemq-->
    <db>hivemq</db>
    <!--    table name default mqtt_payload-->
    <table>mqtt_payload</table>
    <!--    topic column name default topic-->
    <topicColumn>mqtt_topic</topicColumn>
    <!--    payload column name default payload-->
    <PayloadColumn>payload</PayloadColumn>
    <!--    max 'NCHAR' type length   -->
    <maxlength>64</maxlength>
    <!--    whether to use sync lock when using http-->
    <httpLock>false</httpLock>
</tdengine>
